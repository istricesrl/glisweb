{
  "sites": {
    "1": {
      "id": 1,
      "__label__": "%etichetta del sito%",
      "name": {
        "it-IT": "%nome it-IT del sito%"
      },
      "protocols": {
        "%stage del sito%": "%protocollo del sito%"
      },
      "hosts": {
        "%stage del sito%": "%nome host del sito%"
      },
      "domains": {
        "%stage del sito%": "%dominio del sito%"
      }
    }
  },
  "auth": {
    "accounts": {
      "root": {
        "password": "%password di root%"
      }
    }
  },
  "debug": {
    "%stage del sito%": {
      "*" : {
        "log": { "lvl": "%livello di log da 1 a 7%" },
        "report": { "lvl": "%livello di report 2 o 32767%" }
      }
    }
  },
  "mysql": {
    "servers": {
      "default": {
        "address": "%indirizzo IP del server MySQL%",
        "port": "%porta del server MySQL%",
        "username": "%nome utente del server MySQL%",
        "password": "%password del server MySQL%",
        "db": "%nome del database MySQL%"
      }
    },
    "profiles": {
      "%stage del sito%": {
        "servers": [
          "default"
        ]
      }
    }
  },
  "twig": {
    "profiles": {
      "%stage del sito%": {
        "debug": false,
        "cache": "var/cache/twig/"
      }
    }
  },%opzioni%
  "mods": {
    "active": {
      "array": [%moduli%]
    }
  }
}
