    immagini_ibfk_08                | id_categoria_risorse          | categorie_risorse.id          | ON DELETE SET NULL ON UPDATE SET NULL
    immagini_ibfk_09                | id_notizia                    | notizie.id                    | ON DELETE SET NULL ON UPDATE SET NULL
    immagini_ibfk_10                | id_categoria_notizie          | categorie_notizie.id          | ON DELETE SET NULL ON UPDATE SET NULL
