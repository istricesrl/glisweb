    account_gruppi_ibfk_98_nofollow     | id_account_inserimento       | account.id                       | ON DELETE SET NULL ON UPDATE SET NULL
    account_gruppi_ibfk_99_nofollow     | id_account_aggiornamento     | account.id                       | ON DELETE SET NULL ON UPDATE SET NULL

